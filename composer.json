{
    "name": "wp-plugin-matrix-starter/plugin",
    "description": "WP Plugin Matrix - Starter - A WordPress plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "WPPluginMatrixStarter\\": "app/",
            "WPPluginMatrixBoilerPlate\\": "app/",
            "MatrixStarter\\": "app/",
            "TestPlugin\\": "app/",
            "MyAwesomePlugin\\": "app/",
            "MyAmazingPlugin\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}